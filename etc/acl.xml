                        </resource>
                        <resource id="HanStudio_EventPageBuilder::schedule" title="Schedules" translate="title">
                            <resource id="HanStudio_EventPageBuilder::schedule_save" title="Save" translate="title"/>
                            <resource id="HanStudio_EventPageBuilder::schedule_delete" title="Delete"
                                      translate="title"/>
                        </resource>
                        <resource id="HanStudio_EventPageBuilder::speaker" title="Speakers" translate="title">
                            <resource id="HanStudio_EventPageBuilder::speaker_save" title="Save" translate="title"/>
                            <resource id="HanStudio_EventPageBuilder::speaker_delete" title="Delete"
                                      translate="title"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
